{
  "manifest_version": 3,
  "name": "Prompt Manager",
  "version": "0.1",
  "description": "Save and use AI prompts quickly",
  
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
  "service_worker": "background.js"},

  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"]
  
}
